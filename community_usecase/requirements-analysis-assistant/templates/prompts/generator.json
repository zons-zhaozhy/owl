                "内容": {
                    "1. 项目概述": {
                        "1.1 项目背景": "...",
                        "1.2 项目目标": "..."
                    },
                    "2. 功能需求": {
                        "2.1 用户管理": {
                            "需求ID": "F001",
                            "描述": "用户注册和登录功能",
                            "优先级": "高",
                            "详细说明": "..."
                        }
                    }
                }
            }
        }
    ]
} 